import kotlin.collections.List;

CREATE TABLE Movies (
    id INTEGER NOT NULL,
    poster_path TEXT,
    adult INTEGER AS Boolean NOT NULL,
    overview TEXT NOT NULL,
    release_date TEXT NOT NULL,
    genre_ids TEXT As List<Int> NOT NULL,
    original_title TEXT NOT NULL,
    original_langauge TEXT NOT NULL,
    title TEXT NOT NULL,
    backdrop_path TEXT,
    populatiry REAL,
    vote_count INTEGER,
    video INTEGER AS Boolean,
    vote_average REAL
);